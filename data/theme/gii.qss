/*based on qss from xnview*/

QWidget
{
	color:#ccc;
	background-color:#333;
	/*selection-background-color: rgba(100, 100, 255, 128);*/
	
	font-size:12px;
	font-family: 
		/*Recursive Monospace;*/
		lekton0;
		/*Anonymous Pro;*/
	
}

QFrame
{
	border:0;
}

QLabel{
	background-color:transparent;
}

QToolButton
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#444,stop:1#333);
	border: 1px solid #252525;
	border-radius:2px;
	color:#ccc;
	padding:2px;
}

QToolButton::hover
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#444,stop:1#444);
	color:#eee;
}

QPushButton
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#555,stop:1#444);
	border: 1px solid #252525;
	border-radius:1px;
	color:#dcdcdc;
	padding:4px;
	margin:1px;
}

QPushButton::hover
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#7b7b7b,stop:1#676767);
	color:#fff;
}

QPushButton::focus, QToolButton::focus
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#7b7b7b,stop:1#676767);
	color:#fff;
}


QDialogButtonBox > QPushButton,QDialog > QPushButton,QWizard > QWidget > QPushButton
{
	min-width:5em;
	min-height:1.4em;
}

QCheckBox
{
	margin-bottom: 1px;
	margin-top: 4px;
	background-color:none;
}

QCheckBox:focus {
	background-color:#444;
	border:1px dotted #777;
}

QDockWidget
{
	background-color:#464646;
	color:#a5a5a5;
	padding: 1px;
}

QDockWidget::title
{
	background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:0 #333, stop:1 #292929);
	padding-top: 5px;
	padding-left: 5px;
	border: 1px solid #111;
	height: 7px;
}


QHeaderView::section
{
	color:#b9b9b9;
	background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:0 #373737, stop:1 #333);
	border-top:0 solid #000;
	border-right:1 solid #000;
	border-bottom:1px solid #222;
	border-left:1px solid #444;
	padding:2px;
	padding-left:5px;
}

QHeaderView::section:first
{
	border-left:0 solid #000;
}

QHeaderView::down-arrow
{
	image:url(theme:arrow_down_V2.png);
	width:10px;
	height:10px;
	padding-right:1ex;
}

QHeaderView::up-arrow
{
	image:url(theme:arrow_up_V2.png);
	width:10px;
	height:10px;
	padding-right:1ex;
}

QScrollBar
{
	background-color: #333;
}

QScrollBar::add-page,QScrollBar::sub-page
{
	background:none;
}

QScrollBar::horizontal
{
	border:0px solid #303030;
	margin:0 11px;
	height: 10px;
	background-color: #333;
}

QScrollBar::vertical
{
	border:0px solid #303030;
	margin:11px 0;
	width: 10px;
	background-color: #333;
