#AnimatorTrackTree{
	background-color:#222;
	alternate-background-color:#404040;
	font-size: 10px;
}
/*
QTreeView#AnimatorTrackTree::branch {
	border-bottom: 1px solid #292929;
}*/

QTreeView#AnimatorTrackTree {
	color: #ccc;
}

QTreeView#AnimatorTrackTree::branch:has-siblings:!adjoins-item {
	border-image: none;
}

QTreeView#AnimatorTrackTree::branch:has-siblings:adjoins-item {
	border-image: none;
	background: #383838;
	border-left: 1px solid #555;
	border-top: 1px solid #444;
	border-bottom: 1px solid #222;
}

QTreeView#AnimatorTrackTree::branch:!has-children:!has-siblings:adjoins-item {
	border-image: none;
	background: #383838;
	border-left: 1px solid #555;
	border-top: 1px solid #444;
	border-bottom: 1px solid #222;
}

QTreeView#AnimatorTrackTree::branch:has-children:!has-siblings:closed,
QTreeView#AnimatorTrackTree::branch:closed:has-children:has-siblings {
	border-image: none;
	image: url(theme:track_branch_closed.png);
	background: #383838;
	border-left: 1px solid #555;
	border-top: 1px solid #444;
	border-bottom: 1px solid #222;
}

QTreeView#AnimatorTrackTree::branch:open:has-children:!has-siblings,
QTreeView#AnimatorTrackTree::branch:open:has-children:has-siblings  {
	border-image: none;
	image: url(theme:track_branch_open.png);
	background: #383838;
	border-left: 1px solid #555;
	border-top: 1px solid #444;
	border-bottom: 1px solid #222;			
}

QTreeView#AnimatorTrackTree::item{
	border-top: 1px solid #444;
	border-bottom: 1px solid #222;
	background: #383838;
}


QTreeView#AnimatorTrackTree::item:selected:!active
{
	background-color:#334a6e;
	color: #fff;
}

QTreeView#AnimatorTrackTree::item:selected:active
{
	background-color:#334a6e;
	color: #fff;
}

